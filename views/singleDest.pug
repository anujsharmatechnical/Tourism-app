                      h2 Destination Places
                    | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                    .row
                        each vr in images1
                            .col-md-12.animate-box
                                .room-wrap
                                .row
                                    .col-md-6.col-sm-6
                                        img.room-img(style="height:100%;width:100%;border:3px solid black;" src="/images/"+vr.famous)
                                    | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                                    .col-md-6.col-sm-6
                                    .desc
                                        h1(style="color:#2966B8;")=vr.category
                                        | &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                                        p(style="font-size:18px;font-weight:bold;") Visitor Information 
                                        each z in vr.info
                                          p
                                            b="Famous For: "+z.Famous_For
                                            br
                                            b="Ticket: "+z.Ticket
                                            br
                                            b="Opening Timings: "+z.Opening_Timing
                                            br
                                            b="Duration: "+z.Duration
                                            br
                                            b="Things to Do:  "+z.Things_To_Do
